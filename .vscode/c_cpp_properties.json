{
    "configurations": [
        {
            "name": "rv32i",
            "includePath": [
                "${default}",
                "${workspaceFolder}/sw/lib/inc/**"
            ],
            "defines": [
                "__riscv",
                "__riscv_xlen=32",
                "USE_SIGNALS"
            ],
            "compilerPath": "/opt/riscv/bin/riscv32-unknown-elf-g++",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x86",
            "compilerArgs": [
                "-march=rv32i_zicsr",
                "-mabi=ilp32",
                "-Wall"
            ]
        }
    ],
    "version": 4
}
